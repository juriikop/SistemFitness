<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="itemLayoutId" format="reference"/>
    <declare-styleable name="StaticList">
        <attr name="gapBetweenTagsVert" format="reference|dimension"/>
        <attr name="gapBetweenTagsHoris" format="reference|dimension"/>
        <attr name="heightTags" format="reference|dimension"/>
        <attr name="widthTagsMax" format="reference|dimension"/>
        <attr name="itemLayoutId"/>
    </declare-styleable>
    <declare-styleable name="PagerSimple">
        <attr name="itemLayoutId"/>
        <attr name="indicator" format="reference"/>
    </declare-styleable>
    <declare-styleable name="PagerIndicator">
        <attr name="itemId" format="reference"/>
        <attr name="itemSelectId" format="reference"/>
        <attr name="selectOne" format="boolean"/>
        <attr name="diametrItem" format="dimension"/>
    </declare-styleable>
    <!--<declare-styleable name="ChartRing">-->
        <!--<attr name="widthLine" format="reference|dimension"/>-->
    <!--</declare-styleable>-->
    <declare-styleable name="Simple">
        <attr name="mask" format="string"/>
        <attr name="imageId" format="reference"/>
        <attr name="viewId" format="reference"/>
        <attr name="negativeViewId" format="reference"/>
        <attr name="positiveViewId" format="reference"/>
        <attr name="showTime" format="integer"/>
        <attr name="selectorLine" format="reference"/>
        <attr name="heightLine" format="dimension"/>
        <attr name="numberFormat" format="string"/>
        <attr name="dateFormat" format="string"/>
        <attr name="placeholder" format="reference"/>
        <attr name="viewLayoutId" format="string"/>
        <attr name="fadedScreenColor" format="color"/>
        <attr name="textSize" format="dimension"/>
        <attr name="hintTextColor" format="color"/>
        <attr name="textColor" format="color"/>
        <attr name="iconColor" format="color"/>
        <attr name="stringArray" format="reference"/>
        <attr name="alias" format="string"/>
        <attr name="textError" format="string"/>
        <attr name="minValue" format="string"/>
        <attr name="maxValue" format="string"/>
        <attr name="minLength" format="integer"/>
        <attr name="fieldLength" format="integer"/>
        <attr name="typeValidate">
            <enum name="filled" value="0" />
            <enum name="email" value="1" />
        </attr>
        <attr name="swipeViewId" format="reference"/>
        <attr name="swipeRightViewId" format="reference"/>
        <attr name="swipeLeftViewId" format="reference"/>
        <attr name="swipeRightType">
            <enum name="CURTAIN" value="0" />
            <enum name="DRAG" value="1" />
            <enum name="HARMONIC" value="2" />
            <enum name="REMOVE" value="3" />
        </attr>
        <attr name="swipeLeftType">
            <enum name="CURTAIN" value="0" />
            <enum name="DRAG" value="1" />
            <enum name="HARMONIC" value="2" />
            <enum name="REMOVE" value="3" />
        </attr>
    </declare-styleable>
</resources>